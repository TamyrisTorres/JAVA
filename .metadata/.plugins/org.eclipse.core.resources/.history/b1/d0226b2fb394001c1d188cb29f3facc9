package cesar;

import java.util.HashMap;
import java.util.Scanner;

public class ExemploHashMap {

	public static void main(String[] args) {

		Scanner entrada = new Scanner(System.in);

		HashMap<String, Integer> cadastro = new HashMap<String, Integer>();

		cadastro.put("um", 1);
		cadastro.put("dois", 2);
		cadastro.put("três", 3);
		cadastro.put("quatro", 4);
		cadastro.put("cinco", 5);

		System.out.println("Digite um número de um a cinco.");
		String numeroExtenso = entrada.next();

		Integer usuario = 0;
		if (cadastro.containsKey(numeroExtenso)) {
			usuario = cadastro.get(numeroExtenso);
			System.out.println(usuario.intValue());
		}
		
		System.out.println(usuario.intValue() * usuario.intValue());

		entrada.close();
	}
}
