package cesar;

import java.util.HashMap;
import java.util.Map.Entry;
import java.util.Scanner;

public class ExemploHashMap {
	
	@SuppressWarnings({ "unused", "unlikely-arg-type" })
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);

		HashMap<String, Integer> cadastro = new HashMap<String, Integer>();
		
		cadastro.put("um", 1);
		cadastro.put("dois", 2);
		cadastro.put("três", 3);
		cadastro.put("quatro", 4);
		cadastro.put("cinco", 5);
		
		System.out.println("Digite um número de um a cinco.");
		String numeroExtenso = entrada.next();
		
		/*for(String n : cadastro.keySet()) {
			if(cadastro.containsKey(numeroExtenso)){
			System.out.println("ei");
			}else {
				System.out.println("Processo Finalizado");
			}*/
			
			for(Entry<String, Integer> usuario: cadastro.entrySet()) {
				if(cadastro.containsKey(numeroExtenso)){
					System.out.println(usuario.getValue());
					}
			}
			
			entrada.close();
		}
}
		

	

