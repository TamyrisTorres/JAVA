package classe;

public class ProdutoTeste {
	
	public static void main(String[] args) {
		
		Produto p1 = new Produto();
		p1.nome = "Notebok";
		p1.preco = 1800.00;
		p1.desconto = 0.25;
		
		var p2 = new Produto();
		p2.nome = "Caneta Preta";
		p2.preco = 12.56;
		p2.preco = 0.29;
		
		System.out.println(p1.nome);
		System.out.println(p2.nome);
	}

}
