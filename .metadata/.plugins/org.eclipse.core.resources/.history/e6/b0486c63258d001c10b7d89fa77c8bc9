package classe;

public class DataTeste {
	
	public static void main(String[] args) {
		
		Data d1 = new Data();
		d1.dia = 15;
		d1.mes = 12;
		d1.ano = 1987;
		
		Data d2 = new Data();
		d2.dia = 30;
		d2.mes = 10;
		d2.ano = 2012;
		
		System.out.printf("%d/%d/%d\n", d1.mes, d1.mes, d1.ano);
		System.out.printf("%d/%d/%d\n", d2.mes, d2.mes, d2.ano);
	}

}
