package cesar;

import java.util.HashMap;
import java.util.Scanner;

public class ExemploHashMap {
	
	@SuppressWarnings({ "unused", "unlikely-arg-type" })
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);

		HashMap<String, Integer> cadastro = new HashMap<String, Integer>();
		
		cadastro.put("um", 1);
		cadastro.put("dois", 2);
		cadastro.put("três", 3);
		cadastro.put("quatro", 4);
		cadastro.put("cinco", 5);
		
		System.out.println("Digite um número de um a cinco.");
		String numero = entrada.next();
		
		for(String n : cadastro.keySet()) {
			if(cadastro.containsKey(n)){
			System.out.println(cadastro.get(cadastro.values()));
			}else {
				System.out.println("Processo Finalizado");
			}
		}
		

		entrada.close();
		
	}

	

}
